<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>{{ country.name }} Facility</h:title>
    <model>
      <instance>
        <data id="facility-0.19">
          <form_version>0.19</form_version>

          <start_time></start_time>
          <end_time></end_time>
          <survey_date></survey_date>
          <device_id></device_id>
          <subscriber_id></subscriber_id>
          <sim_id></sim_id>
          <phone_number></phone_number>

          <section_name>
            <facility_name></facility_name>
            <facility_district>Please select a facility</facility_district>
          </section_name>
          <section_contact>
            <postal_address></postal_address>
            <phone_number></phone_number>
            <email></email>
          </section_contact>
          <section_respondent>
            <respondent_name></respondent_name>
            <respondent_position></respondent_position>
          </section_respondent>
          <section_location>
            <facility_location></facility_location>
            <distance_to_town></distance_to_town>
            <town_name></town_name>
          </section_location>
          <section_photos>
            <photo1></photo1>
            <photo2></photo2>
            <photo3></photo3>
          </section_photos>
          <section_general>
            <type_of_facility></type_of_facility>
            <facility_type_other></facility_type_other>
            <facility_description></facility_description>
          </section_general>
          <section_services>
            <facility_services></facility_services>
            <facility_services_other></facility_services_other>
            <facility_coverage></facility_coverage>
            <facility_nr_patients></facility_nr_patients>
            <facility_nurses></facility_nurses>
            <facility_doctors></facility_doctors>
          </section_services>
          <section_medicines_list>
            <list_available></list_available>
            <list_not_available_why></list_not_available_why>
            <photo1></photo1>
            <photo2></photo2>
            <photo3></photo3>
          </section_medicines_list>
          <section_comments>
            <comments></comments>
          </section_comments>
        </data>
      </instance>
      <itext>
        <translation lang="eng">
          <text id="/data/section_name:label">
            <value>Facility Location</value>
          </text>
          <text id="/data/section_name/facility_name:label">
            <value>Please enter in the facility name.</value>
          </text>
          <text id="/data/section_name/facility_name:hint">
            <value></value>
          </text>
          <text id="/data/section_name/facility_district:label">
            <value>Please select the district that the health facility is in.</value>
          </text>
          <text id="/data/section_name/facility_district:hint">
            <value></value>
          </text>
          <text id="/data/section_name/facility_district:blank">
            <value>Select a district</value>
          </text>

	  {% for district in districts %}
          <text id="/data/section_name/facility_district:option{{ forloop.counter0 }}">
            <value>{{ district.name }}</value>
          </text>
	  {% endfor %}

          <text id="/data/section_contact:label">
            <value>Facility Contact Details</value>
          </text>
          <text id="/data/section_contact/postal_address:label">
            <value>Please enter in the facility postal address.</value>
          </text>
          <text id="/data/section_contact/postal_address:hint">
            <value>e.g. 43 James Street, Cotswold, Jamestown, 4631</value>
          </text>
          <text id="/data/section_contact/phone_number:label">
            <value>Please enter in the facility contact number.</value>
          </text>
          <text id="/data/section_contact/phone_number:hint">
            <value>Include the international dialing code, e.g. +265112223333. Leave blank if unavailable.</value>
          </text>
          <text id="/data/section_contact/email:label">
            <value>Please enter facility email address.</value>
          </text>
          <text id="/data/section_contact/email:hint">
            <value>e.g. info@myclinic.net. Leave blank if unavailable.</value>
          </text>
          <text id="/data/section_respondent:label">
            <value>Respondent Details</value>
          </text>
          <text id="/data/section_respondent/respondent_name:label">
            <value>Please enter the name of the staff member completing this survey.</value>
          </text>
          <text id="/data/section_respondent/respondent_name:hint">
            <value></value>
          </text>
          <text id="/data/section_respondent/respondent_position:label">
            <value>What is their position?</value>
          </text>
          <text id="/data/section_respondent/respondent_position:hint">
            <value>e.g. facility manager, staff nurse, etc.</value>
          </text>
          <text id="/data/section_location:label">
            <value>Location</value>
          </text>
          <text id="/data/section_location/facility_location:label">
            <value>Please capture GPS Coordinates.</value>
          </text>
          <text id="/data/section_location/facility_location:hint">
            <value>You will need to be outside in order to do this. Also make sure that your GPS reader is switched on (see the troubleshooting guide for details).</value>
          </text>
          <text id="/data/section_location/distance_to_town:label">
            <value>What is the distance (km) to the nearest town?</value>
          </text>
          <text id="/data/section_location/distance_to_town:hint">
            <value>A town is defined as a population &gt; 50,000. If you are currently in a town, enter in 0.</value>
          </text>
          <text id="/data/section_location/town_name:label">
            <value>What is the name of the nearest town?</value>
          </text>
          <text id="/data/section_location/town_name:hint">
            <value></value>
          </text>
          <text id="/data/section_photos:label">
            <value>Photos</value>
          </text>
          <text id="/data/section_photos/photo1:label">
            <value>Please take a photo of the front entrance of the facility.</value>
          </text>
          <text id="/data/section_photos/photo1:hint">
            <value></value>
          </text>
          <text id="/data/section_photos/photo2:label">
            <value>Please take a photo of the dispensary.</value>
          </text>
          <text id="/data/section_photos/photo2:hint">
            <value></value>
          </text>
          <text id="/data/section_photos/photo3:label">
            <value>Take any other photo of the facility that might be important (optional).</value>
          </text>
          <text id="/data/section_photos/photo3:hint">
            <value></value>
          </text>
          <text id="/data/section_general:label">
            <value>General Details</value>
          </text>
          <text id="/data/section_general/type_of_facility:label">
            <value>What type of facility is this?</value>
          </text>
          <text id="/data/section_general/type_of_facility:hint">
            <value></value>
          </text>
          <text id="/data/section_general/type_of_facility:blank">
            <value>Select a facility type</value>
          </text>
          <text id="/data/section_general/type_of_facility:option0">
            <value>Government Hospital</value>
          </text>
          <text id="/data/section_general/type_of_facility:option1">
            <value>Government Rural Health Centre or Dispensary</value>
          </text>
          <text id="/data/section_general/type_of_facility:option2">
            <value>Council Clinic</value>
          </text>
          <text id="/data/section_general/type_of_facility:option3">
            <value>Mission or NGO Hospital</value>
          </text>
          <text id="/data/section_general/type_of_facility:option4">
            <value>Private Hospital</value>
          </text>
          <text id="/data/section_general/type_of_facility:option5">
            <value>Private Pharmacy</value>
          </text>
          <text id="/data/section_general/type_of_facility:option6">
            <value>Other</value>
          </text>
          <text id="/data/section_general/facility_type_other:label">
            <value>If you specified other above, please explain what type of facility this is.</value>
          </text>
          <text id="/data/section_general/facility_type_other:hint">
            <value></value>
          </text>
          <text id="/data/section_general/facility_description:label">
            <value>Please provide a short description of the facility.</value>
          </text>
          <text id="/data/section_general/facility_description:hint">
            <value>(max 100 words)</value>
          </text>
          <text id="/data/section_services:label">
            <value>Services and Population</value>
          </text>
          <text id="/data/section_services/facility_services:label">
            <value>Please indicate which services this facility offers.</value>
          </text>
          <text id="/data/section_services/facility_services:hint">
            <value>Tick all options that apply.</value>
          </text>
          <text id="/data/section_services/facility_services:option0">
            <value>Outpatients</value>
          </text>
          <text id="/data/section_services/facility_services:option1">
            <value>Ante-natal</value>
          </text>
          <text id="/data/section_services/facility_services:option2">
            <value>Well baby clinic</value>
          </text>
          <text id="/data/section_services/facility_services:option3">
            <value>Diabetic Clinic</value>
          </text>
          <text id="/data/section_services/facility_services:option4">
            <value>Dispensary</value>
          </text>
          <text id="/data/section_services/facility_services:option5">
            <value>TB Clinic</value>
          </text>
          <text id="/data/section_services/facility_services:option6">
            <value>HCT/VCT</value>
          </text>
          <text id="/data/section_services/facility_services:option7">
            <value>ARV Dispensing</value>
          </text>
          <text id="/data/section_services/facility_services_other:label">
            <value>Add any other major services offered not listed in the question above.</value>
          </text>
          <text id="/data/section_services/facility_services_other:hint">
            <value></value>
          </text>
          <text id="/data/section_services/facility_coverage:label">
            <value>What is the catchment population of this facility?</value>
          </text>
          <text id="/data/section_services/facility_coverage:hint">
            <value>Approximate number of patients that this facility services.</value>
          </text>
          <text id="/data/section_services/facility_nr_patients:label">
            <value>What is the average number of patients serviced daily at this facility?</value>
          </text>
          <text id="/data/section_services/facility_nr_patients:hint">
            <value></value>
          </text>
          <text id="/data/section_services/facility_nurses:label">
            <value>How many nurses are stationed at this facility?</value>
          </text>
          <text id="/data/section_services/facility_nurses:hint">
            <value></value>
          </text>
          <text id="/data/section_services/facility_doctors:label">
            <value>How many doctors are stationed at this facility?</value>
          </text>
          <text id="/data/section_services/facility_doctors:hint">
            <value></value>
          </text>
          <text id="/data/section_medicines_list:label">
            <value>Medicines List</value>
          </text>
          <text id="/data/section_medicines_list/list_available:label">
            <value>Is a list of medicines procured by the facility available?</value>
          </text>
          <text id="/data/section_medicines_list/list_available:hint">
            <value>If yes, please ask for a copy. This should be delivered to the Tendai project supervisor.</value>
          </text>
          <text id="/data/section_medicines_list/list_available:option0">
            <value>Yes</value>
          </text>
          <text id="/data/section_medicines_list/list_available:option1">
            <value>No</value>
          </text>
          <text id="/data/section_medicines_list/list_not_available_why:label">
            <value>Please explain why the list is not available.</value>
          </text>
          <text id="/data/section_medicines_list/list_not_available_why:hint">
            <value></value>
          </text>
          <text id="/data/section_medicines_list/photo1:label">
            <value>Please take a photo of medicines list.</value>
          </text>
          <text id="/data/section_medicines_list/photo1:hint">
            <value>Ensure that the page is readable (page 1).</value>
          </text>
          <text id="/data/section_medicines_list/photo2:label">
            <value>Please take a photo of medicines list.</value>
          </text>
          <text id="/data/section_medicines_list/photo2:hint">
            <value>Ensure that the page is readable (page 2).</value>
          </text>
          <text id="/data/section_medicines_list/photo3:label">
            <value>Photo of medicines list</value>
          </text>
          <text id="/data/section_medicines_list/photo3:hint">
            <value>Ensure that the page is readable (page 3).</value>
          </text>
          <text id="/data/section_comments:label">
            <value>Comments</value>
          </text>
          <text id="/data/section_comments/comments:label">
            <value>Please provide any additional comments not covered by the previous questions.</value>
          </text>
          <text id="/data/section_comments/comments:hint">
            <value></value>
          </text>
        </translation>
      </itext>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/start_time" type="dateTime"/> 
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/end_time" type="dateTime"/> 
      <!-- <bind jr:preload="today" jr:preloadParams="end" nodeset="/data/survey_date" type="date"/> -->
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/data/device_id" type="string"/> 
      <bind jr:preload="property" jr:preloadParams="subscriberid" nodeset="/data/subscriber_id" type="string"/> 
      <bind jr:preload="property" jr:preloadParams="simserial" nodeset="/data/sim_id" type="string"/>
      <bind jr:preload="property" jr:preloadParams="phonenumber" nodeset="/data/phone_number" type="string"/>

      <bind constraint=". != ''" jr:constraintMsg="Please fill in the facility name" nodeset="/data/section_name/facility_name" required="true()" type="string"/>
      <bind constraint=". != 'blank'" jr:constraintMsg="Please select a district from the list" nodeset="/data/section_name/facility_district" required="true()" type="select1"/>
      <bind constraint=". != ''" jr:constraintMsg="Please enter in the facility postal address" nodeset="/data/section_contact/postal_address" required="true()" type="string"/>
      <bind nodeset="/data/section_contact/phone_number" type="string"/>
      <bind nodeset="/data/section_contact/email" type="string"/>
      <bind nodeset="/data/section_respondent/respondent_name" type="string"/>
      <bind nodeset="/data/section_respondent/respondent_position" type="string"/>
      <bind nodeset="/data/section_location/facility_location" required="true()" type="geopoint"/>
      <bind nodeset="/data/section_location/distance_to_town" type="int"/>
      <bind nodeset="/data/section_location/town_name" type="string"/>
      <bind nodeset="/data/section_photos/photo1" type="binary"/>
      <bind nodeset="/data/section_photos/photo2" type="binary"/>
      <bind nodeset="/data/section_photos/photo3" type="binary"/>
      <bind constraint=". != 'blank'" jr:constraintMsg="Please select a facility type from the list" nodeset="/data/section_general/type_of_facility" required="true()" type="select1"/>
      <bind nodeset="/data/section_general/facility_type_other" type="string"/>
      <bind nodeset="/data/section_general/facility_description" type="string"/>
      <bind nodeset="/data/section_services/facility_services" type="select"/>
      <bind nodeset="/data/section_services/facility_services_other" type="string"/>
      <bind nodeset="/data/section_services/facility_coverage" type="int"/>
      <bind nodeset="/data/section_services/facility_nr_patients" type="int"/>
      <bind nodeset="/data/section_services/facility_nurses" type="int"/>
      <bind nodeset="/data/section_services/facility_doctors" type="int"/>
      <bind nodeset="/data/section_medicines_list/list_available" type="select1"/>
      <bind nodeset="/data/section_medicines_list/list_not_available_why" relevant="(/data/section_medicines_list/list_available = 'no')" type="string"/>
      <bind nodeset="/data/section_medicines_list/photo1" relevant="(/data/section_medicines_list/list_available = 'yes')" type="binary"/>
      <bind nodeset="/data/section_medicines_list/photo2" relevant="(/data/section_medicines_list/list_available = 'yes' and /data/section_medicines_list/photo1 != '')" type="binary"/>
      <bind nodeset="/data/section_medicines_list/photo3" relevant="(/data/section_medicines_list/list_available = 'yes' and /data/section_medicines_list/photo2 != '')" type="binary"/>
      <bind nodeset="/data/section_comments/comments" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="field-list">
      <label ref="jr:itext('/data/section_name:label')"/>
      <input ref="/data/section_name/facility_name">
        <label ref="jr:itext('/data/section_name/facility_name:label')"/>
        <hint ref="jr:itext('/data/section_name/facility_name:hint')"/>
      </input>
      <select1 appearance="minimal" ref="/data/section_name/facility_district">
        <label ref="jr:itext('/data/section_name/facility_district:label')"/>
        <hint ref="jr:itext('/data/section_name/facility_district:hint')"/>
        <item>
          <label ref="jr:itext('/data/section_name/facility_district:blank')"/>
          <value>blank</value>
        </item>

	{% for district in districts %}
        <item>
          <label ref="jr:itext('/data/section_name/facility_district:option{{ forloop.counter0 }}')"/>
          <value>{{ district.value }}</value>
        </item>
	{% endfor %}

      </select1>
    </group>
    <group appearance="field-list">
      <label ref="jr:itext('/data/section_contact:label')"/>
      <input ref="/data/section_contact/postal_address">
        <label ref="jr:itext('/data/section_contact/postal_address:label')"/>
        <hint ref="jr:itext('/data/section_contact/postal_address:hint')"/>
      </input>
      <input ref="/data/section_contact/phone_number">
        <label ref="jr:itext('/data/section_contact/phone_number:label')"/>
        <hint ref="jr:itext('/data/section_contact/phone_number:hint')"/>
      </input>
      <input ref="/data/section_contact/email">
        <label ref="jr:itext('/data/section_contact/email:label')"/>
        <hint ref="jr:itext('/data/section_contact/email:hint')"/>
      </input>
    </group>
    <group appearance="field-list">
      <label ref="jr:itext('/data/section_respondent:label')"/>
      <input ref="/data/section_respondent/respondent_name">
        <label ref="jr:itext('/data/section_respondent/respondent_name:label')"/>
        <hint ref="jr:itext('/data/section_respondent/respondent_name:hint')"/>
      </input>
      <input ref="/data/section_respondent/respondent_position">
        <label ref="jr:itext('/data/section_respondent/respondent_position:label')"/>
        <hint ref="jr:itext('/data/section_respondent/respondent_position:hint')"/>
      </input>
    </group>
    <group appearance="field-list">
      <label ref="jr:itext('/data/section_location:label')"/>
      <input ref="/data/section_location/facility_location">
        <label ref="jr:itext('/data/section_location/facility_location:label')"/>
        <hint ref="jr:itext('/data/section_location/facility_location:hint')"/>
      </input>
      <input ref="/data/section_location/distance_to_town">
        <label ref="jr:itext('/data/section_location/distance_to_town:label')"/>
        <hint ref="jr:itext('/data/section_location/distance_to_town:hint')"/>
      </input>
      <input ref="/data/section_location/town_name">
        <label ref="jr:itext('/data/section_location/town_name:label')"/>
        <hint ref="jr:itext('/data/section_location/town_name:hint')"/>
      </input>
    </group>
    <group appearance="field-list">
      <label ref="jr:itext('/data/section_photos:label')"/>
      <upload mediatype="image/*" ref="/data/section_photos/photo1">
        <label ref="jr:itext('/data/section_photos/photo1:label')"/>
        <hint ref="jr:itext('/data/section_photos/photo1:hint')"/>
      </upload>
      <upload mediatype="image/*" ref="/data/section_photos/photo2">
        <label ref="jr:itext('/data/section_photos/photo2:label')"/>
        <hint ref="jr:itext('/data/section_photos/photo2:hint')"/>
      </upload>
      <upload mediatype="image/*" ref="/data/section_photos/photo3">
        <label ref="jr:itext('/data/section_photos/photo3:label')"/>
        <hint ref="jr:itext('/data/section_photos/photo3:hint')"/>
      </upload>
    </group>
    <group appearance="field-list">
      <label ref="jr:itext('/data/section_general:label')"/>
      <select1 appearance="minimal" ref="/data/section_general/type_of_facility">
        <label ref="jr:itext('/data/section_general/type_of_facility:label')"/>
        <hint ref="jr:itext('/data/section_general/type_of_facility:hint')"/>
        <item>
          <label ref="jr:itext('/data/section_general/type_of_facility:blank')"/>
          <value>blank</value>
        </item>
        <item>
          <label ref="jr:itext('/data/section_general/type_of_facility:option0')"/>
          <value>Government Hospital</value>
        </item>
        <item>
          <label ref="jr:itext('/data/section_general/type_of_facility:option1')"/>
          <value>Government Rural Health Centre or Dispensary</value>
        </item>
        <item>
          <label ref="jr:itext('/data/section_general/type_of_facility:option2')"/>
          <value>Council Clinic</value>
        </item>
        <item>
          <label ref="jr:itext('/data/section_general/type_of_facility:option3')"/>
          <value>Mission or NGO Hospital</value>
        </item>
        <item>
          <label ref="jr:itext('/data/section_general/type_of_facility:option4')"/>
          <value>Private Hospital</value>
        </item>
        <item>
          <label ref="jr:itext('/data/section_general/type_of_facility:option5')"/>
          <value>Private Pharmacy</value>
        </item>
        <item>
          <label ref="jr:itext('/data/section_general/type_of_facility:option6')"/>
          <value>Other</value>
        </item>
      </select1>
      <input ref="/data/section_general/facility_type_other">
        <label ref="jr:itext('/data/section_general/facility_type_other:label')"/>
        <hint ref="jr:itext('/data/section_general/facility_type_other:hint')"/>
      </input>
      <input ref="/data/section_general/facility_description">
        <label ref="jr:itext('/data/section_general/facility_description:label')"/>
        <hint ref="jr:itext('/data/section_general/facility_description:hint')"/>
      </input>
    </group>
    <group appearance="field-list">
      <label ref="jr:itext('/data/section_services:label')"/>
      <select ref="/data/section_services/facility_services">
        <label ref="jr:itext('/data/section_services/facility_services:label')"/>
        <hint ref="jr:itext('/data/section_services/facility_services:hint')"/>
        <item>
          <label ref="jr:itext('/data/section_services/facility_services:option0')"/>
          <value>outpatients</value>
        </item>
        <item>
          <label ref="jr:itext('/data/section_services/facility_services:option1')"/>
          <value>ante-natal</value>
        </item>
        <item>
          <label ref="jr:itext('/data/section_services/facility_services:option2')"/>
          <value>well_baby</value>
        </item>
        <item>
          <label ref="jr:itext('/data/section_services/facility_services:option3')"/>
          <value>diabetic</value>
        </item>
        <item>
          <label ref="jr:itext('/data/section_services/facility_services:option4')"/>
          <value>dispensary</value>
        </item>
        <item>
          <label ref="jr:itext('/data/section_services/facility_services:option5')"/>
          <value>tb</value>
        </item>
        <item>
          <label ref="jr:itext('/data/section_services/facility_services:option6')"/>
          <value>hct/vct</value>
        </item>
        <item>
          <label ref="jr:itext('/data/section_services/facility_services:option7')"/>
          <value>arv</value>
        </item>
